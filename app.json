{
  "expo": {
    "name": "voie-rapide",
    "slug": "voie-rapide",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#023047"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.midas.voie-rapide",
      "jsEngine": "hermes"
    },
    "android": {
      "enableProguardInReleaseBuilds": true,
      "enableShrinkResourcesInReleaseBuilds": true,
      "optimization": {
        "size": "small",
        "images": "small"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#023047"
      },
      "package": "com.midas.voierapide",
      "buildType": "apk"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3ab1dfdb-c951-415d-869f-d48fc60e93aa"
      }
    },
    "owner": "pierrot3112",
    "plugins": [
      [
        "expo-build-properties",
        {
          "Android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ]
  }
}
