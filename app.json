{
  "expo": {
    "name": "voie-rapide",
    "slug": "voie-rapide",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "Voie Rapide",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#023047"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.midas.voierapide"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#023047"
      },
      "package": "com.midas.voierapide"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3ab1dfdb-c951-415d-869f-d48fc60e93aa"
      }
    },
    "owner": "pierrot3112",
   "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "useLegacyPackaging": true,
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResources": true
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#023047",
          "image": "./assets/icon.png",
          "imageWidth": 80
        }
      ]
    ] ,
    "newArchEnabled": true
  }
}